{
  "name": "quartz-tpf",
  "version": "1.0.1",
  "description": "QUaRTz",
  "main": "./src/query-engine.js",
  "bin": {
    "quartz": "./bin/quartz.js"
  },
  "scripts": {
    "postinstall": "git submodule update --init Client.js/ && cd Client.js && npm i --production",
    "lint": "eslint src/",
    "test": "mocha tests/**/*.js --trace-deprecation",
    "doc": "jsdoc -c jsdoc.json README.md src/",
    "start-ldf": "cd tests/data/ && ldf-server ./ldf-config.json 5000 4",
    "build": "browserify -r ./quartz-tpf.js:quartz-tpf  -t [ babelify --presets [ es2015 ] ]   > ./quartz-tpf.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Callidon/quartz-tpf.git"
  },
  "author": "Thomas Minier <thomas.minier@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Callidon/quartz-tpf/issues"
  },
  "homepage": "https://github.com/Callidon/quartz-tpf#readme",
  "dependencies": {
    "commander": "^2.9.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.2",
    "sparqljs": "^1.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "http-proxy": "^1.16.2",
    "jsdoc": "^3.4.3",
    "ldf-server": "^2.2.2",
    "mocha": "^3.2.0"
  }
}
