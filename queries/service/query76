    SERVICE <http://52.33.245.25/watDiv_100/query> {
      {
        SELECT * WHERE { ?v0 <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://db.uwaterloo.ca/~galuc/wsdbm/ProductCategory8>. }
